{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "zzcovid19-reporting-adf"
		},
		"ls_ablob_zzcovid19reportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_zzcovid19reportingsa'"
		},
		"ls_adls_zzcovid19reportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_zzcovid19reportingdl'"
		},
		"ls_adls_zzcovid19reportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zzcovid19reportingdl.dfs.core.windows.net/"
		},
		"tr_Populate_Data_OnCreation_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/717e6461-7cbb-41ec-bbe1-653b0bc6179f/resourceGroups/covid19-reporting-rg/providers/Microsoft.Storage/storageAccounts/zzcovid19reportingsa"
		},
		"ls_http_opendata_ecdc_europe_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"tr_ingest_hospital_data_properties_pl_ingest_ecdc_data_parameters_SourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_data_properties_pl_ingest_ecdc_data_parameters_SinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "zzcovid19-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_zzcovid19reportingsa_connectionString": {
						"value": "[parameters('ls_ablob_zzcovid19reportingsa_connectionString')]"
					},
					"ls_adls_zzcovid19reportingdl_accountKey": {
						"value": "[parameters('ls_adls_zzcovid19reportingdl_accountKey')]"
					},
					"ls_adls_zzcovid19reportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_zzcovid19reportingdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europe_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europe_eu_properties_typeProperties_url')]"
					},
					"tr_Populate_Data_OnCreation_properties_typeProperties_scope": {
						"value": "[parameters('tr_Populate_Data_OnCreation_properties_typeProperties_scope')]"
					},
					"tr_ingest_hospital_data_properties_pl_ingest_ecdc_data_parameters_SourceRelativeURL": {
						"value": "[parameters('tr_ingest_hospital_data_properties_pl_ingest_ecdc_data_parameters_SourceRelativeURL')]"
					},
					"tr_ingest_hospital_data_properties_pl_ingest_ecdc_data_parameters_SinkFileName": {
						"value": "[parameters('tr_ingest_hospital_data_properties_pl_ingest_ecdc_data_parameters_SinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}